{
    "name": "familytree365/laravel-dna",
    "description": "A package that converts dna files to Eloquent models",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "familytree365/php-dna": "^1.0",
        "doctrine/dbal": "^2.8|^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FamilyTree365\\LaravelDna\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FamilyTree365\\LaravelDna\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "rector/rector": "^0.11.49"
    }
}
